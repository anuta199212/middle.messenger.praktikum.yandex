<html>

<head>
    <title>Menu</title>
    {{> meta}}
</head>

<body>
    <nav class="menu">
        <ul class="menu-list">
            {{#each navigation.pages}}
            <li class="menu-item">
                <a href="{{this.url}}">{{this.title}}</a>
            </li>
            {{/each}}
        </ul>
    </nav>
</body>

</html>
